<body>
	<!--HEADER-->
		<header>
			<div class="timer">
		        {{exercise.name}}
		        <input type="hidden" id="name" value={{exercise.name}}>
		        <input type="hidden" id="exer_interval" value={{exercise.interval}}>
		        <input type="hidden" id="total_exer" value={{exercise.total_exercises}}>

	            <div id = "time_display">
	                00:00:00
	            </div>
	            <div class="buttons">
	                <button id="startStop" onclick="startStop()">GO</button>
	                <button id="reset" onclick="reset()">Reset</button>
	            </div>
    		</div>
		</header>


		<!--Perset Workouts-->
		<div class="myexercises">
			
			<button id="addworkout_btn"><a href="add">Add Workout</a></button>
			<h3>History</h3>
			<ul id="all_exer">
				<li>Hello</li>
			</ul>
		</div>

		<div class="home-btn">
			<ul>
				{{#each Nav}}
				<button id="app_btn"><a href="{{path}}">{{name}}</a></button>
				{{/each}}
			</ul>
		</div>








