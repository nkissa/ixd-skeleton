<body>
	<div class="timer">
		<div id = "time_display">
			00:00:00
		</div>
		<div class="buttons">
			<button id="startStop" onclick="startStop()">GO</button>
			<button id="reset" onclick="reset()">Reset</button>
		</div>
	</div>
	<div id="list-exer">
	
	</div>

	<form class="interval_container" method="POST" id="data_form" action="/add">
			<label id="input_timer">Workout Name:</label>
			<input type="text" id="exer_name" onclick="clear()">
			<br>
			<label id="input_timer">Interval per Minute(1-10):</label>
			<br>
			<input type="number" id="input" onclick="clear()" min="1" max="10">
	    	<!--button id="input_btn" onclick="intervalTime()">Submit</button-->
	    	<br>
	    	<label id="exer_title">How many exercises are you doing(Choose 1-6)</label>
			<br>
			<input type="number" id="exer_input" onclick="clear()" min="1" max="6">
			<br>
	       <button id="exer_btn">Submit</button>
	</form>











	<div class="home-btn">
		<ul>
			{{#each Nav}}
				<button id="app_btn"><a href="{{path}}">{{name}}</a></button>
			{{/each}}
		</ul>
	</div>


</body>
