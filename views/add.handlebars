<html>
<head>
	<title>HCI Project Portfolio</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/main.css" rel="stylesheet">
	{{> google}}
</head>
<body>
	<header>
			<div id="app_title">
				<h1>Add Workout</h1>
			</div>
		</header>

	<form class="interval_container" method="POST" id="data_form" action="/add">
			<label id="input_timer">Workout Name:</label>
			<br>
			<input type="text" id="exer_name" onclick="clear()">
			<br>

			
			<label id="input_timer">Interval per Minute(1-10):</label>

			<label id="input_timer">Minutes Per Interval(1-10):</label>

			<br>
			<input type="number" id="input" onclick="clear()" min="1" max="10">
	    	<!--button id="input_btn" onclick="intervalTime()">Submit</button-->
	    	<br>
	    	<label id="exer_title">How many exercises are you doing(Choose 1-6)</label>
			<br>
			<input type="number" id="exer_input" onclick="clear()" min="1" max="6">
			<br>
	       <button id="exer_btn"class="btn btn-primary btn-lg btn-block">Submit</button>
	</form>











	<div class="home-btn">
		<ul>
			{{#each Nav}}
				<button id="app_btn"><a href="{{path}}">{{name}}</a></button>
			{{/each}}
		</ul>
	</div>


</body>
<script src="https://code.jquery.com/jquery.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="js/script.js" type="text/javascript"></script>
